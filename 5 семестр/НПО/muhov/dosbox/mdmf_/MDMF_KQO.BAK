SELECT MDMF_C.*, MDMF_K.*;
 FROM MDMF_K, MDMF_C;
 WHERE MDMF_C.MDMF_C0 = MDMF_K.MDMF_K0;
   AND MDMF_K.MDMF_KDATA >= MDMF_C.MDMF_CDATS;
   AND MDMF_K.MDMF_KDATA < MDMF_C.MDMF_CDATD;
   AND MDMF_K.MDMF_KS = mdmf_c.mdmf_cs;
 INTO CURSOR MDMF_KQO_A
REPORT FORM MDMF_KRO.FRX TO FILE MDMF_KFO.TXT NOCONSOLE NOEJECT

